{
  "name": "frontend",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "start": "npm run create-link && astro dev",
    "build": "npm run remove-link && astro check && astro build",
    "build:ci": "astro check && astro build",
    "preview": "astro preview",
    "astro": "astro",
    "remove-link": "@powershell -NoProfile -ExecutionPolicy Unrestricted -Command ../tools/src/symlink.ps1 -Action remove",
    "create-link": "@powershell -NoProfile -ExecutionPolicy Unrestricted -Command ../tools/src/symlink.ps1 -Action create",
    "metadata": "cd ../tools && venv\\Scripts\\activate && python src/exif.py generate && cd ../frontend",
    "clean": "cd ../tools && venv\\Scripts\\activate && python src/exif.py clean && cd ../frontend",
    "import": "python ../tools/src/images.py import",
    "deploy": "aws s3 sync ./dist s3://www.kevinverreault.com --delete",
    "deploy-images": "aws s3 sync ../image-sources s3://kevinverreault-photos/images --delete"
  },
  "dependencies": {
    "@astrojs/check": "^0.8.3",
    "@astrojs/react": "^3.6.0",
    "@fancyapps/ui": "^5.0.33",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "astro": "^4.12.2",
    "cva": "npm:class-variance-authority@^0.7.0",
    "formik": "^2.4.5",
    "posthog-js": "^1.105.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.4.1",
    "react-router-dom": "^6.22.0",
    "typescript": "^5.3.3",
    "yup": "^1.3.3"
  },
  "devDependencies": {
    "@types/node": "^20.11.17",
    "node-html-parser": "^6.1.12"
  }
}

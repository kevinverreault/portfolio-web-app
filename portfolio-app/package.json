  "scripts": {
    "start": "react-scripts start",
    "build": "npx eslint src/** && react-scripts build",
    "clean-build": "npx eslint src/** && npm run metadata && npm run overwrite && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "npx eslint src/** --fix",
    "deploy": "node ../deployment/dist/deploy.js",
    "deploy-sources": "node ../deployment/dist/deploy.js -s",
    "metadata": "node ../deployment/dist/metadata.js",
    "overwrite": "node ../exif-utility/src/exif-overwrite.js",
    "refresh": "node ../deployment/dist/image-utilities.js",
    "refresh-all": "node ../deployment/dist/image-utilities.js -a",
    "release": "npm run refresh-all && npm run clean-build && npm run deploy"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/fancybox": "^3.5.3",
